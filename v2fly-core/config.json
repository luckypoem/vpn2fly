{
    "log": {
	"access": "/etc/v2ray/access.log",
	"error": "/etc/v2ray/error.log",
        "loglevel": "warning"
    },
    "inbounds": [{
            "listen": "10.13.128.1",
            "port": 1024,
            "protocol": "vmess",
            "settings": { "clients": [{ "id": "${YOUR-V2RAY-UUID}" }]},
            "streamSettings": {
                "network": "ws",
                "wsSettings": { "path": "/lazy" }
            }
    }],
    "outbounds": [{ "protocol": "freedom" }]
}